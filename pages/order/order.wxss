.tab{
	position:fixed;
	left:0;
	top:0;

	width:750rpx;
	height:89rpx;
	box-sizing: border-box;
	border-top:1rpx solid #c1c1c1;
	border-bottom:1rpx solid #dfdfdd;
	background-color:#fff;

	display: flex;
	flex-flow: row nowrap ;
	justify-content: space-around;
	align-items: center;

	color:#666666;
	font-size:10pt;
	font-weight:400;
	line-height:89rpx;
	text-align:center;
	z-index:1000;	
}
